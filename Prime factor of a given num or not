import java.util.Scanner;
class Main
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt(),i,sum=0,c=0;
		for(i=1;i<=n;i++)
		{
		    if(n%i==0)
		    {
		    	c++;
		    }
		}
		if(c==2)
		{
	    	System.out.println("yes");
		}
		else
		{
		    System.out.println("no");
		}
	}
}
