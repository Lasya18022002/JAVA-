   import java.util.*;
class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt(),i,j,k;
	    for(i=n,k=1;i>=1;i--,k++)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(i<=j)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	        }
	          for(j=2;j<=n;j++)
	          {
	            if(k>=j)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	          }
	          System.out.println();
	    }
	    	    for(i=2,k=n-1;i<=n;i++,k--)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(i<=j)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	        }
	          for(j=2;j<=n;j++)
	          {
	            if(k>=j)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	          }
	          System.out.println();
	    }
	}
}
