0001

import java.util.*;
class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt(),i,j;
	    for(i=n;i>=1;i--)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(j==n)
	            {
	    	      System.out.print("1");
	            }
	            else if(i==1)
	            {
	                System.out.print("0");
	            }
	            else if(i==j)
	            {
	                System.out.print("2");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	        }
	        System.out.println();
	    }
	}
}

