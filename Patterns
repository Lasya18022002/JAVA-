	    int n=sc.nextInt(),i,j;
	    for(i=n;i>=1;i--)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(i>=j)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	        }
	        System.out.println();
	    }
	}
