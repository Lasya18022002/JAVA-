import java.util.*;
class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt(),i,j;
	    for(i=1;i<=n;i++)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(i==j)
	            {
	    	      System.out.print("1");
	            }
	            else if(i>=j)
	            {
	                System.out.print("0");
	            }
	        }
	        System.out.println();
	    }
	}
}

