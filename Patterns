import java.util.*;
class Main
{
	public static void main(String[] args)
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt(),i,j;
	    for(i=n;i>=1;i--)
	    {
	        for(j=1;j<=n;j++)
	        {
	            if(i==j||j==n||i==1)
	            {
	    	      System.out.print("*");
	            }
	            else
	            {
	                System.out.print(" ");
	            }
	        }
	        System.out.println();
	    }
	}
}

