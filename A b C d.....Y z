import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        char ch;
        for(ch='A';ch<='Z';++ch)
            {
                if(ch%2==0)
                {
                    System.out.print((char)(ch+32)+" ");
                }
                else if(ch%2==1)
                {
                    System.out.print(ch+" ");
                }
            }
    }
}    
