import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),c=0,i=1;
        while(i<=n)
        {
           if(n%i==0) 
           c++;
           i++;
        }
        if(c%2!=0)
        {
        System.out.print("Yes");
        }
        else
        {
            System.out.print("No");
        }
    } 
}
