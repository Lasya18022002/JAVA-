import java.util.Scanner;
class Main
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt(),i,sum=0;
		for(i=1;i<=n;i++)
		{
		    if(n%i==0)
		    {
		       sum=sum+i;
		    }
		}
		    if(sum-n==n)
		    {
		    	System.out.println("perfect");
	    	}
	    	else
            {
		    	System.out.println(" not perfect");
	       	}
	}
}
