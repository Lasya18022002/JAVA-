import java.util.*;
class Main
{
    public static void main(String args[])
    {
        int a,b,c;
        Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        System.out.println((a>b&&a>c)?(b>c)?(b):(c):(b>a&&b>c)?(a>c)?(a):(c):(c>a&&c>b)?(a>b)?(a):(b));
    }
}    
