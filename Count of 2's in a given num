import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),c=0,r;      
        while(n>0)
        {
            r=n%10;                      
            if(r==2)                
            c++;                   
            n=n/10;                      
        }
        System.out.print(c);
    } 
}
