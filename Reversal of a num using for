import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),i,rev=0,r;       
        for(i=1;i<=n;)            
        {
          r=n%10;                   
          rev=rev*10+r;            
          n=n/10;                  
        }
        System.out.print(rev);
    }
}    
