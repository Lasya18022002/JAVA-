import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),i,c=1;
        for(i=1;c<=n;i++)
        {
            if(i%2==0)
            {
                System.out.print(i+" ");
                c++;
            }
        } 
    }
}    
