import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a;
        a=sc.nextInt();
        if(12%a==0&&a%2==1)
        {
            System.out.println("1");
        }
        else
        {
            System.out.println("0");
        }
    }
}
